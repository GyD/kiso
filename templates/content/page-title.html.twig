{#
/**
 * @file
 * Kiso's theme override for page titles.
 *
 * You will find same variables as in the core 'page-title.html.twig' template.
 *
 * Related WCAG resources:
 * - SC 1.3.1 Info and Relationships (Level A):
 *   - H42: Using h1-h6 to identify headings.
 */
#}
{{ title_prefix }}
{% if title %}
  <h1{{ title_attributes.addClass('page__title') }}>{{ title }}</h1>
{% endif %}
{{ title_suffix }}
