{#
/**
 * @file
 * Default theme implementation for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 *
 */
#}
{% if breadcrumb %}
  <nav class="breadcrumb" aria-labelledby="breadcrumb" role="navigation">
    <h2 id="breadcrumb" class="visually-hidden">{{ 'You are here:'|t }}</h2>
    <ol>
    {% for item in breadcrumb %}
      <li>
        {% if item.url %}
          <a href="{{ item.url }}" title="{{ item.text }}">{{ item.text }}</a>
        {% else %}
          {{ item.text }}
        {% endif %}
      </li>
    {% endfor %}
    </ol>
  </nav>
{% endif %}